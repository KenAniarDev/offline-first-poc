    "env-cmd": "^10.1.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "formik": "^2.4.4",
    "next": "^12",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.29",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2",
    "yup": "^1.2.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "nodemon": "^3.0.1"
